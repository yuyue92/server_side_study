{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "default",
  "description": "Default permissions for the music player",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "core:window:allow-close",
    "core:window:allow-minimize",
    "core:window:allow-maximize",
    "core:app:default",
    "core:event:default",
    "core:path:default",
    "dialog:default",
    "dialog:allow-open",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-read-dir",
    "fs:allow-exists"
  ]
}